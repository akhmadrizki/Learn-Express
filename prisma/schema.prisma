datasource db {
    provider = "sqlite"
    url      = "file:../db/dev.db"
}

generator client {
    provider = "prisma-client-js"
}

model Shoe {
    id        Int      @id @default(autoincrement())
    name      String
    brand     String
    qty       Int
    available Boolean  @default(true)
    desc      String
    price     Int
    img       String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
